<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GradeView">

    <TextView
        android:id="@+id/txtQz4GV"
        android:layout_width="140dp"
        android:layout_height="42dp"
        android:layout_marginEnd="56dp"
        android:hint="@string/quiz_4GV"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtQz2GV"
        app:layout_constraintVertical_bias="0.059" />

    <TextView
        android:id="@+id/txtQz3GV"
        android:layout_width="140dp"
        android:layout_height="42dp"
        android:hint="@string/quiz_3GV"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtQz4GV"
        app:layout_constraintHorizontal_bias="0.733"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtQz1GV"
        app:layout_constraintVertical_bias="0.062" />

    <TextView
        android:id="@+id/txtTitleGV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/grade_viewGV"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.08" />

    <TextView
        android:id="@+id/txtFullNameGV"
        android:layout_width="300dp"
        android:layout_height="42dp"
        android:hint="@string/full_nameGV"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTitleGV"
        app:layout_constraintVertical_bias="0.052" />

    <TextView
        android:id="@+id/txtAttendanceGV"
        android:layout_width="140dp"
        android:layout_height="42dp"
        android:hint="@string/attendanceGV"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.793"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider14"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/txtQz1GV"
        android:layout_width="140dp"
        android:layout_height="42dp"
        android:hint="@string/quiz_1GV"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtQz2GV"
        app:layout_constraintHorizontal_bias="0.733"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtExamGV"
        app:layout_constraintVertical_bias="0.048" />

    <TextView
        android:id="@+id/txtExamGV"
        android:layout_width="140dp"
        android:layout_height="42dp"
        android:hint="@string/examGV"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtAttendanceGV"
        app:layout_constraintHorizontal_bias="0.733"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtStatusGV"
        app:layout_constraintVertical_bias="0.109" />

    <TextView
        android:id="@+id/txtQz2GV"
        android:layout_width="140dp"
        android:layout_height="42dp"
        android:layout_marginEnd="56dp"
        android:hint="@string/quiz_2GV"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAttendanceGV"
        app:layout_constraintVertical_bias="0.051" />

    <TextView
        android:id="@+id/txtAverageGV"
        android:layout_width="300dp"
        android:layout_height="42dp"
        android:hint="@string/averageGV"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtFullNameGV"
        app:layout_constraintVertical_bias="0.04" />

    <TextView
        android:id="@+id/txtStatusGV"
        android:layout_width="300dp"
        android:layout_height="42dp"
        android:hint="@string/statusGV"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAverageGV"
        app:layout_constraintVertical_bias="0.031" />

    <Button
        android:id="@+id/btnMenuGV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.17"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider16"
        app:layout_constraintVertical_bias="0.305" />

    <View
        android:id="@+id/divider12"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toTopOf="@+id/txtFullNameGV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

    <View
        android:id="@+id/divider14"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toTopOf="@+id/txtExamGV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtStatusGV"
        app:layout_constraintVertical_bias="0.314" />

    <View
        android:id="@+id/divider16"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtRemarks"
        app:layout_constraintVertical_bias="0.176" />

    <TextView
        android:id="@+id/txtRemarks"
        android:layout_width="300dp"
        android:layout_height="42dp"
        android:hint="@string/remarksGV"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtQz3GV"
        app:layout_constraintVertical_bias="0.08" />

</androidx.constraintlayout.widget.ConstraintLayout>
